import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int temp=n;
        int sum=0;
        int count=0;
        while(temp>0){
            temp=temp/10;
            count++;
        }
        while(temp>0){
            int digit=temp%10;
            sum=sum+(int) Math.pow(digit,count);
            temp=temp/10;
        }
        if(temp==sum){
            System.out.print("Armstrong number");
        }
        else{
            System.out.print("not a armstrong number");
        }
    }
}
