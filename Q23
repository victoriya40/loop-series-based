import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        while(n>9){
            int sum=0;
            while(n>0){
                sum=sum+(n%10);
                n=n/10;
            }
            n=sum;
        }
        if(n==1){
            System.out.print("magic number");
        }
        else{
            System.out.print("not a magic number");
        }
    }
}
