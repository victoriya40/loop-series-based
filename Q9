import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
         int n=sc.nextInt();
         int oddsum=0;
         int evensum=0;
        for(int i=1;i<=n;i++){
            if(i%2==0)
              evensum+=i;
              else
               oddsum+=i;
        }
        System.out.println("Odd sum: "+oddsum);
         System.out.println("Even sum: "+evensum);
        }
    }
